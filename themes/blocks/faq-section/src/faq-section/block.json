{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/faq-section",
	"version": "0.1.0",
	"title": "Faq Section",
	"category": "theme",
	"icon": "admin-comments",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "faq-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"tag": {
			"type": "string"
		},
		"heading": {
			"type": "string"
		},
		"paragraph": {
			"type": "string"
		},
		"contact": {
			"type": "object",
			"properties": {
				"heading": {
					"type": "string"
				},
				"phone": {
					"type": "string"
				},
				"email": {
					"type": "string"
				}
			}
		},
		"openWeekHours": {
			"type": "object",
			"properties": {
				"startDate": {
					"type": "string"
				},
				"endDate": {
					"type": "string"
				}
			}
		},
		"openWeekendHours": {
			"type": "object",
			"properties": {
				"startDate": {
					"type": "string"
				},
				"endDate": {
					"type": "string"
				}
			}
		},
		"faq": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"heading": {
						"type": "string"
					},
					"paragraph": {
						"type": "string"
					}
				}
			}
		}
	}
}