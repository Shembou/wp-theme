{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/cards-section",
	"version": "0.1.0",
	"title": "Cards Section",
	"category": "theme",
	"icon": "grid-view",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "cards-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"cards": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"heading": {
						"type": "string"
					},
					"HeaderType": {
						"type": "string",
						"default": "h1"
					},
					"pins": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"button": {
						"type": "object",
						"properties": {
							"url": {
								"type": "string"
							},
							"text": {
								"type": "string"
							},
							"svg": {
								"type": "string",
								"default": ""
							},
							"alt": {
								"type": "string"
							}
						}
					},
					"cornerImage": {
						"type": "string"
					},
					"cornerImage_alt": {
						"type": "string"
					},
					"color": {
						"type": "string",
						"enum": [
							"gray",
							"blue-green"
						]
					}
				}
			}
		}
	}
}