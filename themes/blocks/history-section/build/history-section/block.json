{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/history-section",
  "version": "0.1.0",
  "title": "History Section",
  "category": "common",
  "icon": "welcome-view-site",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "textdomain": "history-section",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "tag": {
      "type": "string"
    },
    "history_showcase": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "heading": {
            "type": "string"
          },
          "paragraph": {
            "type": "string"
          },
          "year": {
            "type": "string"
          }
        }
      }
    }
  }
}