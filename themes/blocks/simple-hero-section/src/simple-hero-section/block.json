{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/simple-hero-section",
	"version": "0.1.0",
	"title": "Simple Hero Section",
	"category": "common",
	"icon": "text-page",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "simple-hero-section",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"heading": {
			"type": "string"
		},
		"paragraph": {
			"type": "string"
		},
		"image": {
			"type": "string"
		},
		"button": {
			"type": "object",
			"properties": {
				"url": {
					"type": "string"
				},
				"text": {
					"type": "string"
				},
				"svg": {
					"type": "string",
					"default": ""
				}
			}
		}
	}
}